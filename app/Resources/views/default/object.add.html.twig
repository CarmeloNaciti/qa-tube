{% extends 'base.html.twig' %}
{% form_theme form 'bootstrap_3_layout.html.twig' %}

{% block body %}
    <body>
    <div class="container">
        <div class="row">
            <div class="col-xs-4">
                <div class="demo-content"></div>
            </div>
            <div class="col-xs-8">
                <div class="demo-content bg-alt">
                    {{ form_start(form) }}
                    {{ form_row(form.mediaFile) }}
                    {{ form_row(form.story) }}
                    <div id="storyError"></div>
                    {{ form_widget(form) }}
                    <input class="btn btn-info" type="submit" value="Submit" />
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>
    </body>
    <script src="{{ asset('js/video/add.js') }}"></script>
{% endblock body %}